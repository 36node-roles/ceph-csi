---
apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  name: rook-ceph
  annotations:
    kubernetes.io/ingress.class: "{{ ingress_class }}"
spec:
  rules:
    - host: "ceph.{{ domain }}"
      http:
        paths:
          - backend:
              serviceName: rook-ceph-mgr-dashboard
              servicePort: 7000
            path: /
